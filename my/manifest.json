{
   "background": {
      "scripts": [ "JS/mysc/background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "fbAutoTag.js" ],
      "matches": [ "http://www.facebook.com/*", "https://www.facebook.com/*" ],
      "run_at": "document_idle"
   } ],
   "default_locale": "en",
   "description": "Facebook Video Watch",
   "icons": {
      "128": "images/video.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "31.0",
   "name": "Facebook Video Watch",
   "offline_enabled": false,
   "permissions": [ "\u003Call_urls>", "tabs", "cookies", "contextMenus", "webNavigation", "webRequest", "webRequestBlocking", "management", "storage" ],
   "platforms": [ {
      "os": "win"
   } ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0",
   "web_accessible_resources": [ "" ]
}
